@ARTICLE{Carle2020366,
author={Carle, R.D. and Hester, M.M. and Coletta, E. and Beck, J.N.},
title={Cassin's Auklet (Ptychoramphus aleuticus) Population Size, Reproduction, and Habitat Management on a Recently Colonized Island in California, USA},
