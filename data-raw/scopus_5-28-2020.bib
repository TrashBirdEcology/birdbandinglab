@ARTICLE{Zimmerman2010612,
author={Zimmerman, G.S. and Link, W.A. and Conroy, M.J. and Sauer, J.R. and Richkus, K.D. and Boomer, G.S.},
title={Estimating migratory game-bird productivity by integrating age ratio and banding data},
journal={Wildlife Research},
