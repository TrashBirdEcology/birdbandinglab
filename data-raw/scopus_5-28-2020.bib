@ARTICLE{Rosche20191226,
author={Rosche, S.B. and Moorman, C.E. and Pacifici, K. and Jones, J.G. and DePerno, C.S.},
title={Northern bobwhite breeding season habitat selection in fire-maintained pine woodland},
journal={Journal of Wildlife Management},
year={2019},
